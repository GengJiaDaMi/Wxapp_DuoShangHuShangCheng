.item {
    width: 100%;
    height: 150rpx;
    box-sizing: border-box;
    padding: 20rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-bottom: 1rpx solid #d7d7d7;
}

.item image:first-child {
    width: 110rpx;
    height: 110rpx;
    border-radius: 5rpx;
    overflow: hidden;
    display: block;
}

.item image:last-child {
    width: 50rpx;
    height: 50rpx;
    border-radius: 5rpx;
    overflow: hidden;
    display: block;
}

.item view {
    width: 460rpx;
}

navigator {
    width: 230rpx;
    height: 65rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 0 0 20rpx 20rpx;
    background: #39a336;
    color: white;
    border-radius: 6rpx;
}