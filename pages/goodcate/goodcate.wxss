.fadedcolor {
    background: linear-gradient(#d7d7d7, #fff);
    background-color: #fff;
    position: fixed;
    height: 15rpx;
    width: 100%;
    z-index: 2;
}

.icon_top {
    width: 85rpx;
    height: 85rpx;
    position: fixed;
    bottom: 300rpx;
    right: 25rpx;
}

.icon_top image {
    width: 85rpx;
    height: 85rpx;
    display: block;
}

.no_data {
    width: 100%;
    display: flex;
    justify-content: space-around;
    align-items: center;
    color: #a8a8a8;
    height: 70rpx;
    font-size: 28rpx;
    padding: 0 80rpx;
    box-sizing: border-box;
}

.no_data::after,
.no_data::before {
    content: '';
    display: block;
    width: 80rpx;
    height: 1rpx;
    background: #e1e1e1;
}

.numoncar {
    position: absolute;
    right: 0;
    bottom: 45rpx;
    width: 35rpx;
    height: 35rpx;
    background-color: #ff5546;
    color: #fff;
    text-align: center;
    line-height: 35rpx;
    border-radius: 100%;
    font-size: 24rpx;
}

.accounts {
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 99;
    width: 750rpx;
    align-items: center;
    background-color: #fff;
    box-shadow: 0px 0px 30px 0px rgba(183, 183, 183, 0.4);
    font-size: 26rpx;
    justify-content: space-between;
}

.checkAll {
    width: 15%;
    align-items: center;
}

.text-normal {
    font-size: 26rpx;
    color: #333;
}

.checkbox {
    width: 90rpx;
    height: 100rpx;
    margin-top: 8rpx;
    position: relative;
    margin-left: 30rpx;
}

.account {
    width: 32%;
    flex: 1.5;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.cricle {
    width: 40rpx;
    height: 40rpx;
    background-color: #ff5545;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 100%;
    position: absolute;
    bottom: 50rpx;
    left: 40rpx;
}

.payaccount {
    width: 56%;
    display: flex;
    justify-content: space-between;
    margin-right: 1%;
    line-height: 50rpx;
}

.goodcar {
    width: 65rpx;
    height: 65rpx;
    margin-top: 20%;
}

.balance {
    background: #ff5546;
    color: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 210rpx;
    height: 110rpx;
}

.nobalance {
    width: 210rpx;
    height: 110rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #c8c8c8;
    color: #fff;
}

.colorr {
    color: #ff5546;
    font-weight: bold;
}

.colorg {
    color: #bbb;
    margin-right: 10rpx;
}

.flex {
    display: flex !important;
    display: -webkit-flex !important;
}

.diamond {
    width: 15rpx;
    height: 37rpx;
    margin-right: 10rpx;
    background-color: #39a336;
}

.sendpdt {
    width: 120rpx;
    height: 45rpx;
    font-size: 25rpx;
    color: #39a336;
    display: flex;
    background-color: #f2f2f2;
    align-items: center;
    justify-content: center;
    border-radius: 15rpx;
    font-weight: bold;
    padding-bottom: 2rpx;
}

.goods-text {
    height: 120rpx;
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    flex-direction: column;
}

.oldprice {
    display: flex;
    margin-left: 10%;
    font-size: 26rpx;
    align-items: flex-end;
    color: #a1a1a1;
    text-decoration: line-through;
}

.hei150 {
    font-weight: bold;
    color: #434343;
}

.shopcar {
    width: 65rpx;
    height: 65rpx;
    position: absolute;
    right: 0;
    bottom: 0;
}

.nodiamond {
    width: 15rpx;
    height: 50rpx;
    margin-right: 10rpx;
    background-color: #fff;
}

.search-wrapper {
    margin-top: 15rpx;
    position: fixed;
    width: 100%;
    height: 90rpx;
    display: flex;
    border-bottom: 2rpx solid #eee;
    background: #fff;
    z-index: 2;
}

.search-input {
    width: 100%;
    height: 70rpx;
    border-radius: 18px;
    background: #f0f0f0;
    padding: 0 20rpx 0 90rpx;
    font-size: 13px;
    margin-left: 3%;
    margin-right: 3%;
}

.icon-search {
    position: absolute;
    left: 50rpx;
    width: 50rpx;
    height: 50rpx;
    top: 10rpx;
}

.fadedf {
    position: fixed;
    top: 0;
    z-index: 2;
    background: white;
}

.category-wrapper {
    width: 100%;
    height: calc(100% - 238rpx);
    overflow: scroll;
    position: fixed;
}

.sidebar-wrapper {
    overflow: scroll;
    height: 100%;
    background-color: #fcfcfc;
    position: fixed;
    font-size: 30rpx;
}

.category-item {
    width: 160rpx;
    height: 110rpx;
    display: flex;
    align-items: center;
    color: #a1a1a1;
    border-bottom: 1rpx solid #dedede;
}

.result-wrapper {
    background: #fff;
    position: absolute;
    right: 0;
    width: 590rpx;
    height: 100%;
    overflow: scroll;
}

.hover {
    background: #fff;
}

.active {
    background: #fff;
    color: #39a336;
}

.goods-item {
    display: flex;
    align-items: center;
    height: 180rpx;
    border-bottom: 1rpx solid #e8e8e8;
    margin: 0 25rpx;
    padding: 30rpx 0;
}

.goods-item:last-child {
    border-bottom: none;
}

.goods-image {
    width: 180rpx;
    height: 180rpx;
    margin-right: 20rpx;
    border-radius: 10rpx;
}

.goods-info {
    display: flex;
    flex: 1;
    height: 180rpx;
    flex-direction: column;
    justify-content: space-between;
    padding-right: 20rpx;
    position: relative;
}


