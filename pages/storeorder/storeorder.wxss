.top-header {
    border-bottom: 0;
}

page {
    height: 100%;
}

.container {
    width: 100%;
    padding-bottom: 75rpx;
    height: calc(100% - 75rpx);
}

.ping {
    position: fixed;
    background-color: rgba(178, 178, 178, 0.8);
    width: 100%;
    height: 100%;
    top: 0;
}

.hei40 {
    padding: 10rpx 30rpx;
    font-size: 31rpx;
}

.centhei40 {
    width: 50%;
    height: 80rpx;
    text-align: center;
}

.pingtop {
    background-color: #fff;
    width: 88%;
    margin-left: 6%;
    margin-top: 30%;
    padding-top: 20rpx;
}

.colorg {
    color: #39a336;
    border-left: 1rpx solid #f4f4f4;
}

.pingbottom {
    display: flex;
    width: 88%;
    margin-left: 6%;
    background-color: #fff;
    justify-content: space-between;
    border-top: 1rpx solid #f4f4f4;
}

.img {
    width: 40rpx;
    height: 40rpx;
}

.status-box {
    width: 100%;
    line-height: 100rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background-color: #fff;
}

.status-box .status-label {
    width: 150rpx;
    height: 100%;
    text-align: center;
    font-size: 32rpx;
    color: #353535;
    box-sizing: border-box;
    position: relative;
}

.status-box .status-label.active {
    color: #38a435;
    font-weight: bold;
    /* border-bottom: 6rpx solid #38a435; */
}

.status-box .status-label .red-dot {
    width: 16rpx;
    height: 16rpx;
    position: absolute;
    left: 125rpx;
    top: 20rpx;
    background-color: #b5272d;
    border-radius: 50%;
}

.no-order {
    width: 100%;
    height: 100%;
    text-align: center;
    background-color: #f2f2f2;
    box-sizing: border-box;
    padding-top: 88rpx;
}

.no-order-img {
    width: 220rpx;
    height: 220rpx;
    margin-bottom: -10rpx;
    margin-top: 30%;
}

.no-order .text {
    font-size: 28rpx;
    color: #999;
    text-align: center;
}

.order-list {
    width: 710rpx;
    margin: 0 20rpx;
}

.order-list .a-order {
    width: 100%;
    background-color: #fff;
    margin-top: 20rpx;
}

.order {
    width: 100%;
    height: 420rpx;
    border-top: 1rpx solid #d7d7d7;
}

.order-title {
    width: 100%;
    height: 128rpx;
    display: flex;
    justify-content: space-between;
}

.order-address {
    width: 450rpx;
    float: left;
    height: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.order-address text {
    width: 380rpx;
    height: 70rpx;
    align-items: flex-start;
}

.order-phone {
    font-size: 26rpx;
    float: left;
    width: 220rpx;
    height: calc(100 - 35rpx);
    display: flex;
    justify-content: left;
    align-items: flex-start;
    margin-top: 35rpx;
}

.order-phone image {
    width: 35rpx;
    height: 35rpx;
    margin-right: 5rpx;
}

.order-address image {
    width: 72rpx;
    height: 72rpx;
    border-radius: 10rpx;
    overflow: hidden;
    display: block;
    margin-right: 10rpx;
}

.texttwoline {
    font-size: 26rpx;
    display: flex;
    align-items: flex-start;
}

.order-details {
    width: 100%;
    height: 155rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.order-details-img {
    width: 475rpx;
    height: 155rpx;
    font-size: 28rpx;
}

.order-details-img view {
    width: 135rpx;
    height: 155rpx;
    display: flex;
    justify-content: space-between;
    flex-direction: column;
    align-items: center;
    float: left;
    margin-right: 25rpx;
}

.order-details-img view:last-child {
    margin: unset;
}

.order-details-text {
    font-size: 28rpx;
    width: 235rpx;
    height: 155rpx;
    display: flex;
    justify-content: flex-end;
    align-items: flex-end;
    flex-direction: column;
}

.order-details-text text {
    display: block;
    height: 50%;
    display: flex;
    align-items: flex-end;
    font-size: 28rpx;
    font-weight: bold;
}

.textoneline {
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    word-wrap: break-word;
    white-space: normal !important;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.order-details-img view image {
    width: 135rpx;
    height: 110rpx;
    border-radius: 10rpx;
}

.order-list .a-order .order-date {
    padding: 0 30rpx;
    height: 70rpx;
    display: flex;
    justify-content: space-between;
    font-size: 26rpx;
    color: #000;
    align-items: center;
}

.order-list .a-order .order-date .red {
    font-size: 26rpx;
    color: #b5272d;
}

.a-order .goods-info,
.goods-img-container {
    width: 100%;
    border-top: 1rpx solid #f5f5f5;
    padding: 16rpx 0 16rpx 0;
    display: flex;
    align-items: center;
    color: #333;
}

.goods-info .img-box {
    width: 120rpx;
    height: 120rpx;
    overflow: hidden;
    margin-right: 30rpx;
    background-color: #f7f7f7;
}

.goods-info .img-box .goods-img,
.goods-img-container .img-box .goods-img {
    width: 120rpx;
    height: 120rpx;
}

.goods-info .goods-des {
    width: 95%;
    line-height: 39rpx;
    font-size: 26rpx;
    color: #000;
    overflow: hidden;
    margin-left: 30rpx;
}

.goods-img-container {
    height: 180rpx;
    box-sizing: border-box;
    white-space: nowrap;
    border-bottom: 1px solid #f5f5f5;
    padding-left: 30rpx;
}

.goods-img-container .img-box {
    width: 120rpx;
    height: 120rpx;
    overflow: hidden;
    margin-right: 20rpx;
    background-color: #f7f7f7;
    display: inline-block;
}

.order-list .a-order .price-box {
    position: relative;
    width: 720rpx;
    height: 100rpx;
    margin-left: 30rpx;
    box-sizing: border-box;
    padding: 20rpx 30rpx 20rpx 0;
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 26rpx;
}

.order-list .a-order .price-box .btn-box {
    display: flex;
    justify-content: flex-end;
}

.order-list .a-order .price-box .total-price {
    font-size: 26rpx;
    color: #b5272d;
}

.a-order .price-box .btn {
    width: 166rpx;
    height: 60rpx;
    box-sizing: border-box;
    text-align: center;
    line-height: 60rpx;
    border-radius: 6rpx;
    margin-left: 20rpx;
}

.a-order .price-box .cancel-btn {
    border: 1rpx solid #d7d7d7;
    position: absolute;
    right: 216rpx;
    top: 20rpx;
    color: #999;
}

.a-order .price-box .topay-btn {
    border: 1px solid #b5272d;
    color: #b5272d;
}

.confirm-btn {
    background: #fff;
    border: 1px solid #b5272d;
    float: right;
}

.toeva-btn {
    background: #fff;
    border: 1px solid #b5272d;
    float: right;
}

.order-ctrl {
    height: 140rpx;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.btn {
    margin: unset;
    padding: 30rpx;
    height: 70rpx;
    display: flex!important;
    line-height: 1rem;
    justify-content: center;
    align-items: center;
    margin-left: 25rpx;
    color: #333 !important;
}

.btn-wihte {
    color: white !important;
}

.pingbody {
    width: 88%;
    background-color: #fff;
    margin-top: 30%;
    margin-left: 6%;
    border-radius: 20rpx;
}

.pingbottom {
    display: flex;
    border-top: 1rpx solid #f4f4f4;
    height: 100rpx;
    align-items: center;
}

textarea {
    width: 100%;
    border: 1rpx solid #d7d7d7;
    height: 150rpx;
    padding: 15rpx;
    border-radius: 10rpx;
    box-sizing: border-box;
    margin-bottom: 15rpx;
}

.fuck {
    width: 160rpx;
    height: 60rpx;
    background-color: #39A336;
    border-radius: 25px;
    float: right;
    margin-right: 25rpx;
    font-size: 28rpx;
    text-align: center;
    line-height: 60rpx;
    color: #FFFFFF;
    font-weight: bold;
    margin-left: unset;
}

.fuck_all {
    width: 160rpx;
    height: 60rpx;
    background-color: #FFFFFF;
    border-radius: 25px;
    float: right;
    margin-right: 25rpx;
    font-size: 28rpx;
    text-align: center;
    margin-left: unset;
    line-height: 60rpx;
    color: #777777;
    background: #f5f5f5;
    font-weight: bold;
}