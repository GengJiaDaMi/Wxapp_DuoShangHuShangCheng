@import '/pages/managshop/managshop.wxss';
.search_box view {
    width: 120rpx;
    height: 100%;
    color: #39a336;
    display: flex;
    align-items: center;
    justify-content: center;
}

.item {
    width: 100%;
    height: 230rpx;
    box-sizing: border-box;
    padding-right: 20rpx;
    border-bottom: 2rpx solid #ededed;
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.item>image {
    width: 170rpx;
    height: 170rpx;
    display: block;
}

.no_data {
    width: 239rpx;
    height: 256rpx;
    display: block;
    margin: 0 auto;
}

.no_data_box {
    display: flex;
    flex-direction: column;
    justify-content: flex-end;
    align-items: center;
    height: 500rpx;
}